{
  "name": "react-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/react-app/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project react-app --web",
  "targets": {
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "react-app:build",
        "port": 3000
      }
    },
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "react-app:build",
        "port": 3000
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "dist/apps/react-app",
        "configFile": "apps/react-app/vite.config.ts"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "buildTarget": "react-app:build",
        "port": 4173
      }
    }
  }
}
